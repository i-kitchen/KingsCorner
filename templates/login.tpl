<div id="login" class="vcenter">
    <div class="header">
        <h2>Kings Corners</h2>
    </div>
    <form class="login" onsubmit="return validateLogin(this);">

        <div class="container">
            <label><b>Who is playing?</b></label>
            <input type="text" placeholder="Enter Username" name="uname" title="3 to 15 alpha-numeric characters only" required>

            <label><b>Password</b></label>
            <input type="password" placeholder="Enter Password" name="psw" required>

            <button type="submit" class="loginBtn">Join</button>
        </div>

    </form>
</div>